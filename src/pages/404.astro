---
import Layout from '../components/Layout.astro'
Astro.response.status = 404
export const prerender = true
---

<Layout title="找不到页面" description="你要找的页面也许并不存在。" index>
  <div flex="~ 1 col items-center justify-center">
    <h1 text="32 sm:48" font="mono 800" select="none" line-height="none">
      404
    </h1>
    <p text="4 sm:6" p="y-4" select="none">找不到页面</p>
  </div>
</Layout>
